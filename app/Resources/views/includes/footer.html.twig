<!-- BEGIN FOOTER -->
<div id="footer" class="section section_content section-footer center">
	{% set photo_section_footer = '' %}
	{% if photosFooter|length > 0 %}
		{% set photo_section_footer = random(photosFooter) %}
		{% set photo_section_footer = photo_section_footer.webPath %}
	{% else %}
		{% set photo_section_footer = "img/bg/6.jpg" %}
	{% endif %}
	<div class="section bg_photo_changing" style="background-image: url('{{ asset(photo_section_footer) }}'); background-size: cover; background-position: 50%;">
		<!-- Jssor Slider Begin -->
	    <!-- You can move inline styles to css file or css block. -->
	        <!-- Slides Container -->
	    <!-- <div id="slider_container_footer" class="slider_container_bg">
	        <div u="slides" id="slider_inner_footer" class="slider_inner_bg">
				{# if photos|length > 0 %}
					{% for single_photo in photos %}
						{% if single_photo.category == 'footer' %}
							<div>
								<img u=image src="{{ asset(single_photo.webPath) }}" />
							</div>
						{% endif %}
					{% endfor %}
				{% endif #}
	        </div>
	    </div> -->
	    <!-- Trigger -->
	    <script>
	        // jssor_slider_starter('slider_container_footer');
	    </script>
	    <!-- Jssor Slider End -->
	</div>

	<div class="inner-section inner-section-footer row">
		<div class="row">
			<div class="unshow_photo_bg float-right" onClick="showFrame(true);">
				<span class="glyphicon glyphicon-chevron-left"></span> {{ 'main.actions.show' | trans }}
			</div>
		</div>

		<div class="row">
			<div class="col-lg-2 col-md-1 col-sm-1 hidden-xs"></div>
			<div class="col-lg-8 col-md-10 col-sm-10 col-xs-12 bg_frame bg_frame_footer">
				<div class="show_photo_bg float-right" onClick="showFrame(false);">
					{{ 'main.actions.hide' | trans }} <span class="glyphicon glyphicon-chevron-right"></span>
				</div>
				<div class="bg_frame_content">
					<h1>
						{{ 'header.titles.contact' | trans }}
					</h1>

					<div class="row">
						<form id="contact_form" method="post" action="{{ path('bridges_main_footer_contact') }}">
					        <div id="" class="row">

					        	<div class="form-group row">
									<div class="input-icon col-lg-6 col-md-6 col-sm-6 col-xs-12">
										<input type="text" name="senderContact" id="senderContact" placeholder="{{ 'footer.contact.senderName' | trans }}" class="form-control placeholder-no-fix"/>
							        </div>
									<div class="input-icon col-lg-6 col-md-6 col-sm-6 col-xs-12">
										<input type="text" name="emailContact" id="emailContact" placeholder="{{ 'footer.contact.senderEmail' | trans }}" class="form-control placeholder-no-fix"/>
							        </div>
								</div>
								<br/>
					            <div class="form-group row">
					            	<div class="input-icon col-lg-12 col-md-12 col-sm-12 col-xs-12">
						                <select name="subjectMail" id="objet" class="form-control placeholder-no-fix">
						                    <option value="" selected>
						                    	{{ 'footer.contact.selectObject' | trans }}
						                    </option>
						                    <option value="{{ 'footer.contact.object1' | trans }}">
						                    	{{ 'footer.contact.object1' | trans }}
						                    </option>
											<option value="{{ 'footer.contact.object2' | trans }}">
												{{ 'footer.contact.object2' | trans }}
											</option>
											<option value="{{ 'footer.contact.object3' | trans }}">
												{{ 'footer.contact.object3' | trans }}
											</option>
											<option value="{{ 'footer.contact.object4' | trans }}">
												{{ 'footer.contact.object4' | trans }}
											</option>
											<option value="{{ 'footer.contact.object5' | trans }}">
												{{ 'footer.contact.object5' | trans }}
											</option>
											<option value="{{ 'footer.contact.object6' | trans }}">
												{{ 'footer.contact.object6' | trans }}
											</option>
										</select>
					            	</div>
					          	</div> 	
								<br/>
					            <div class="form-group row">
					                <div class="input-icon col-lg-12 col-md-12 col-sm-12 col-xs-12">
					                	<textarea class="form-control placeholder-no-fix" name="corpsMail" id="corpsMail" placeholder="{{ 'footer.contact.message' | trans }}"></textarea>
					                </div>
					            </div>
					        </div>
					        <br/>
					        <input type="submit" value="{{ 'footer.actions.send' | trans }}" class="btn btn-footer uppercase">
					    </form>
					</div>
				</div>
				
			</div>
			<div class="col-lg-2 col-md-1 col-sm-1 hidden-xs"></div>
		</div>

		<br/><br/>


		<div class="footer_copyright row">
			<a href="{{ path('bridges_user_homepage') }}" class="no_style_a">
				{{ 'footer.bottom.definition' | trans }} Â© {{ 'footer.bottom.allRightsReserved' | trans }} 2015
			</a>
		</div>

	</div>

</div>
<!-- END FOOTER -->